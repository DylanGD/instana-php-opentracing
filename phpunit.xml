<?xml version="1.0" ?>
<phpunit
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/6.5/phpunit.xsd"
        bootstrap="vendor/autoload.php">
    <testsuites>
        <testsuite name="default">
            <directory>test/</directory>
        </testsuite>
    </testsuites>
    <groups>
        <exclude>
            <group>end2end</group>
        </exclude>
    </groups>
</phpunit>
